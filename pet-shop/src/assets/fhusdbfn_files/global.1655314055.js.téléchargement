 yet..."),r+=1,1!=c.variants.length&&1!=u.confirmed){console.log("Product has NOT been confirmed, prompting for input..."),g.Modal.dialog("variant",{title:"Choose Your Options",product:c,buttons:[{type:"primary block",label:"Add to Cart",action:"callback",callback:function(e){e.displayed.data.buttons[0].label="Adding...",u.variant_id=c.selected_variant.id,u.confirmed=!0,console.log("= = = Choose your options = = = :",c.title),console.log("Current selected variant",c.selected_variant.title),console.log("product ID",c.id),console.log("variant ID",c.selected_variant.id),console.log("product_state",u),t.saveSettings(),t.addItems()}}]});break}console.log("This product HAS been confirmed..."),u.variant_id=c.selected_variant.id,u.confirmed=!0,n.push({id:c.selected_variant.id,quantity:c.quantity,properties:{_source:"Rebuy",_promo_id:t.id,_attribution:"Marketing Link"}}),t.saveSettings()}}console.log("Promo.addItems - count","items: ",n.length,"unredeemed: ",r),n.length&&n.length==r?(console.log("We have a count match!"),"redirect"==t.settings.success_behavior.type&&(t.appendLoadingIcon(),i&&(i.skip_open=!0)),t.settings.clear_cart&&"automatic"==t.settings.type&&(console.log("= = = = = CLEARING CART!"),e.clearCart()),console.log("= = = = = ADDING ITEMS TO CART!"),console.log(n),e.addItem({items:n},(function(){"redirect"==t.settings.success_behavior.type&&i&&setTimeout((function(){delete i.skip_open}),500),t.successBehavior(),t.status="ready"}))):(t.status="ready",console.log("Promo.addItems complete - Status reset!"))}else console.log("Promo is currently working...",t.status)},f.Promo.prototype.hasTriggeredLandingBehavior=function(){var t=!1;return this.settings.states.landing_behavior.displayed&&(t=!0),console.log("= = = = => Promo.hasTriggeredLandingBehavior",t),console.log(this.settings.states),t},f.Promo.prototype.recordTriggeredLandingBehavior=function(){this.settings.states.landing_behavior.displayed=!0,this.saveSettings()},f.Promo.prototype.landingBehavior=function(){i