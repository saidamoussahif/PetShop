ata,t.customer=e.customer&&e.customer,t.settings=e.settings&&e.settings,t.discount=e.discount&&e.discount[0],t.page_id=e.page_id&&e.page_id,t.error=!1,e.custom_template&&(t.custom_template=e.custom_template),s.ReactivateLandingPage.callbacks.init(t),t.init())}),(function(e){if(e&&e.responseJSON)if(e.responseJSON.error)t.error=e.responseJSON.error;else if(e.responseJSON.errors)if("string"==typeof e.responseJSON.errors)t.error=e.responseJSON.errors;else{var i=[];for(let t in e.responseJSON.errors)i.push(e.responseJSON.errors[t]);t.error=i.join(",")}t.init(),console.log(e,"Reactivate Landing Page Error")}))},f.ReactivateLandingPage.prototype.init=function(){var t=this,e=window.Rebuy,i=f.Utilities,r=f.Page.context(),o=f.Event,s=i.getParameter("subscription_ids");if(t.error){c={el:"#rebuy-reactivate-landing-page",data:t,mounted:function(){n(".rebuy-loader-contain").css("display","none")}};t.getTemplate(),t.view=new a(c)}else{f.Event.ReactivateLandingPage.callbacks.beforeReady(),t.status="initializing",t.initialCancelledSubscriptionCount=0,t.cancelledSubscriptionCount=0,t.template={},t.sortCancelledSubscription(),t.modal={loading:!1,visible:!1,step:1,subscription:{},data:[],selectedAddressId:"",selectedAddress:{},replicateAddress:{},message:{status:"",text:""}};for(let e of t.subscriptions)e.condition="ready","admin_preview"==r&&(e.status="CANCELLED"),"cancelled"==e.status.toLowerCase()&&(t.cancelledSubscriptionCount+=1,t.initialCancelledSubscriptionCount+=1),i.initializeProduct(e.product,!0),i.selectVariantByID(e.product,e.shopify_variant_id);t.custom_template||t.getTemplate();var c={el:"#rebuy-reactivate-landing-page",data:t,methods:{sizeImage:i.fnWrap(t.sizeImage,t),handleReactivateSubmission:i.fnWrap(t.handleReactivateSubmission,t),variantOnSale:i.fnWrap(i.variantOnSale,t),formatMoney:i.fnWrap(t.formatMoney,t),selectVariant:i.fnWrap(i.selectVariant,t),frequencyLabel:i.fnWrap(t.frequencyLabel,t),variantCompareAtPrice:i.fnWrap(i.variantCompareAtPrice,t),hasProductReview